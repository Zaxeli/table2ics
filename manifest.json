{
    "manifest_version": 2,
  
    "name": "Table to calendar",
    "description": "get ics file from table",
    "version": "1.0",
  
    "browser_action": {
        "default_icon": "./icons/icon64.png",
        "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["./scripts/content.js"]
    }],
    "icons": {
        "256": "./icons/icon256.png"
    }
  }
  